<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.section2.sceneswitchdemo.ManageRoomsController">

    <children>
        <Text layoutX="220" layoutY="50" text="Manage Rooms">
            <font><Font size="24" /></font>
        </Text>

        <TableView fx:id="roomsTable" layoutX="70" layoutY="80" prefHeight="240" prefWidth="460">

            <columns>
                <TableColumn fx:id="colRoom" prefWidth="80" text="Room" />
                <TableColumn fx:id="colType" prefWidth="120" text="Type" />
                <TableColumn fx:id="colStatus" prefWidth="120" text="Status" />
                <TableColumn fx:id="colAction" prefWidth="120" text="Action" />
            </columns>

        </TableView>
      <Button layoutY="-5.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="60.0" onAction="#Back" text="Back">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
